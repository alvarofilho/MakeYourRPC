{
  "name": "MakeYourRPC",
  "version": "1.3.5",
  "description":
    "Create your Rich Presence on Discord in an easy and practical way.",
  "main": "main.js",
  "scripts": {
    "start": "yarn sass && electron .",
    "pack": "build --dir",
    "dist": "yarn sass && build",
    "sass": "node-sass -o src/css/ src/sass/"
  },
  "repository": "git@github.com:SrSheep/MakeYourRPC.git",
  "homepage": "https://github.com/SrSheep/MakeYourRPC#readme",
  "keywords": ["Electron", "Discord", "RPC"],
  "author":
    "√Ålvaro Filho <alvaro.fduarte@hotmail.com> (https://github.com/SrSheep/MakeYourRPC)",
  "license": "MIT",
  "dependencies": {
    "discord-rpc": "./RPC",
    "electron-settings": "^3.2.0",
    "jquery": "^3.3.1",
    "jsdom": "^11.10.0"
  },
  "devDependencies": {
    "electron": "2.0.2",
    "electron-builder": "^20.13.3",
    "node-sass": "^4.9.0"
  },
  "build": {
    "appId": "com.makeyourrpc",
    "win": {
      "target": "nsis",
      "icon": "/src/img/512x512.png"
    },
    "linux": {
      "target": ["AppImage", "snap"]
    }
  }
}
